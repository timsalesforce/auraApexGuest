	cash.extend = function (target, ...objs) {
	    const length = arguments.length;
	    for (let i = (length < 2 ? 0 : 1); i < length; i++) {
	        for (const key in arguments[i]) {
	            target[key] = arguments[i][key];
	        }
	    }
	    return target;
	};
	fn.extend = function (plugins) {
