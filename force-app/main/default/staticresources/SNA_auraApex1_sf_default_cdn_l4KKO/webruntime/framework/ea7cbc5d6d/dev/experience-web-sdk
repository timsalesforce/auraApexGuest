	each(['width', 'height'], (index, prop) => {
	    fn[prop] = function (value) {
	        if (!this[0])
	            return isUndefined(value) ? undefined : this;
	        if (!arguments.length) {
	            if (isWindow(this[0]))
	                return this[0][camelCase$2(`outer-${prop}`)];
