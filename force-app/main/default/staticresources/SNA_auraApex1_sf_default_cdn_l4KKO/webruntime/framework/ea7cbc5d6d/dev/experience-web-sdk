	function matches(ele, selector) {
	    const matches = ele && (ele['matches'] || ele['webkitMatchesSelector'] || ele['msMatchesSelector']);
	    return !!matches && matches.call(ele, selector);
	}
	cash.matches = matches;
	function isCash(x) {
	    return x instanceof Cash;
	}
	function isWindow(x) {
	    return !!x && x === x.window;
